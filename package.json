    "scripts": {
        "front:install": "cd front && yarn install",
        "front:start": "cd client && yarn dev",
        "server:install": "source env/bin/activate && cd api && pip install -r requirements.txt",
        "server:start": "source env/bin/activate && cd api && python manage.py runserver",
        "server:migrate": "cd api && python manage.py migrate",
        "server:makemigrations": "cd api && python manage.py makemigrations",
        "server:createsuperuser": "cd api && python manage.py createsuperuser"
    },
